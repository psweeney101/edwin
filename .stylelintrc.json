{
  "$schema": "http://json.schemastore.org/stylelintrc",
  "extends": [
    "@psweeney101/stylelint-config"
  ],
  "ignoreFiles": "**/dist/**",
  "rules": {
    "selector-pseudo-element-no-unknown": [
      true,
      {
        "ignorePseudoElements": [
          "ng-deep"
        ]
      }
    ]
  }
}