{
  "name": "edwin",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "nx run-many --target build --configuration production",
    "containerize": "docker build -t edwin .",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2020 browser module main",
    "lint": "nx run-many --target lint && stylelint **/*.{css,scss} && nx workspace-lint && sort-package-json --check",
    "lint:fix": "nx run-many --target lint --fix && stylelint **/*.{css,scss} --fix && nx workspace-lint && sort-package-json",
    "ng": "nx",
    "prepare": "husky install",
    "start": "nx run-many --target serve",
    "test": "nx run-many --target test",
    "test:watch": "nx run-many --target test --watch"
  },
  "dependencies": {
    "@angular/animations": "~14.1.1",
